{
  "manifest_version": 3,
  "name": "Image Converter",
  "description": "Convert and download images in different formats (PNG, JPG, WEBP).",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
